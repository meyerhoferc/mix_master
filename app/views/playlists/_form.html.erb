<%= form_for(@playlist) do |f| %>
  <%= render partial: 'shared/errors', locals: { target: @playlist } %>
  <%= f.text_field :name %>

  <% @songs.each do |song| %>
    <div>
      <%= check_box_tag(:song_ids, song.id, false,
        :name => 'playlist[song_ids][]', id: "song-#{song.id}") %>
    </div>
  <% end %>
  <%= f.submit %>
<% end %>
